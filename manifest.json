{
    "manifest_version": 3,
    "name": "TabFlux",
    "version": "1.0.0",
    "description": "Beautiful new tabs with facts and Unsplash images", 
    "chrome_url_overrides": {
        "newtab": "./extension/newtab.html"
    },
    "permissions": [
        "storage"
    ],
    "action" : {
        "default_title": "TabFlux"
    },
    "background": {
        "service_worker": "./extension/background.js"
    },
    "host_permissions": ["https://facts-chrome-extension.onrender.com/*"]
}