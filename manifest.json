{
    "manifest_version": 3,
    "name": "TabFlux - New Tab with Fun & Educational Facts",
    "version": "1.0.0",
    "description": "A minimal new tab page that shows random facts and stunning images â€” a smarter way to browse.", 
    "chrome_url_overrides": {
        "newtab": "./extension/newtab.html"
    },
    "icons":{
        "16": "./extension/logo.png"
    }, 
    "permissions": [
        "storage", "search"
    ],
    "action" : {
        "default_icon":{
            "16": "./extension/logo.png"
        },
        "default_title": "TabFlux"
    },
    "background": {
        "service_worker": "./extension/background.js"
    },
    "host_permissions": ["https://facts-chrome-extension.onrender.com/*"]
}